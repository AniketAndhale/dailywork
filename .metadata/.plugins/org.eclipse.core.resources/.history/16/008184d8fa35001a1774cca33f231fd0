package aniket12;

public class Lambda {
public static void main(String[] args) {
	Hello hello=new Hello() {
		public void display() {
			System.out.println("this is inner class");
		}
	};
	hello.display();
	
	Hello h=()-> System.out.println("this is lambda");
	h.display();
}
}
interface Hello
{
	public void display();
	
}
